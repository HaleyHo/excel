worksheet\cf7 .\cf5 write(\cf8 2\cf5 , \cf8 0\cf5 , \cf8 1234.56\cf5 , cell_format02)         \cf4 # -> 1,235\cf5 \
\
cell_format03\cf7 .\cf5 set_num_format(\cf11 '#,##0.00'\cf5 )\
